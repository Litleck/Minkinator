{
	"name": "minkinator",
	"author": "Litleck",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Litleck/Minkinator.git"
	},
	"dependencies": {
		"@discordjs/opus": "^0.3.2",
		"canvas": "^2.6.1",
		"chalk": "^4.1.0",
		"chart.js": "^2.9.3",
		"discord.js": "^12.2.0",
		"entities": "^2.0.3",
		"gif-encoder": "^0.7.2",
		"mathjs": "^7.2.0",
		"moment": "^2.27.0",
		"node-fetch": "^2.6.1",
		"pluralize": "^8.0.0",
		"pretty-bytes": "^5.3.0",
		"pretty-ms": "^7.0.0",
		"qrcode": "^1.4.4",
		"sequelize": "^6.3.4",
		"sqlite3": "^5.0.0",
		"twemoji": "^13.0.1"
	},
	"devDependencies": {
		"eslint": "^7.7.0",
		"prettier": "2.1.1"
	},
	"eslintConfig": {
		"env": {
			"commonjs": true,
			"es2020": true,
			"node": true
		},
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaVersion": 11
		},
		"rules": {
			"indent": [
				"error",
				2
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"quotes": [
				"error",
				"double"
			],
			"semi": [
				"error",
				"always"
			]
		}
	}
}
