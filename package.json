{
  "name": "minkinator",
  "author": "Litleck",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Litleck/Minkinator.git"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "canvas": "^2.6.1",
    "chalk": "^4.1.0",
    "chart.js": "^2.9.3",
    "discord.js": "^12.5.1",
    "entities": "^2.0.3",
    "mathjs": "^7.2.0",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.1",
    "pluralize": "^8.0.0",
    "pretty-bytes": "^5.3.0",
    "pretty-ms": "^7.0.0",
    "purpl-markov-chain": "^2.0.3",
    "sequelize": "^6.3.4",
    "sqlite3": "^5.0.0",
    "twemoji": "^13.0.1",
    "ytdl-core-discord": "^1.2.4"
  },
  "devDependencies": {
    "eslint": "^7.17.0"
  },
  "main": "./src/index.js",
  "scripts": {
    "start": "node .",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 11
    },
    "globals": {
      "util": "readonly",
      "Discord": "readonly"
    },
    "rules": {
      "prefer-destructuring": [
        "error",
        {
          "object": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}
